{
  "name": "hyperspan",
  "version": "0.0.2",
  "description": "Hyperspan CLI - for @hyperspan/framework",
  "type": "module",
  "public": true,
  "publishConfig": {
    "access": "public"
  },
  "main": "dist/index.js",
  "types": "src/index.ts",
  "exports": {
    ".": {
      "types": "./src/index.ts",
      "default": "./dist/index.js"
    }
  },
  "bin": {
    "hyperspan": "dist/index.js"
  },
  "author": "Vance Lucas <vance@vancelucas.com>",
  "license": "BSD-3-Clause",
  "homepage": "https://www.hyperspan.dev",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vlucas/hyperspan.git"
  },
  "bugs": {
    "url": "https://github.com/vlucas/hyperspan/issues"
  },
  "scripts": {
    "test": "bun test",
    "build": "bun build src/index.ts --outdir dist --target node",
    "prepack": "npm run build"
  },
  "dependencies": {
    "effect": "^3.17.3",
    "@effect/cli": "^0.69.0",
    "@effect/platform": "^0.90.0",
    "@effect/platform-bun": "^0.77.0"
  },
  "devDependencies": {
    "@effect/language-service": "^0.31.2"
  }
}
